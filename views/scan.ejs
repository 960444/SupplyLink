<div class="card card-body order-card">
  <span style="color:#ffab00; font-size: 24px; text-align: center;"><b>SCAN BATCH</b></span>
  <form onSubmit="App.createBatch(); return false;">
      <p>Order id:</p><input class="form-control" id="order_id" type="text">
      <p>Quantity:</p><input class="form-control" id="quantity" type="text">
      <p>Cost:</p><input class="form-control" id="cost" type="text">
      <p>Product:</p><input class="form-control" id="product" type="text">
      <p>Manufacturer:</p><input class="form-control" id="bmanufacturer" type="text">
      <p>Recipient:</p><input class="form-control" id="recipient" type="text">
      <p><div id="error"> </div>
      <button style="margin-left: 100px;"type="submit" class="btn btn-primary btn-sml custom-btn">SCAN BATCH</button>
  </form>
</div>
<div class="card card-body order-card">
  <span style="color:#ffab00; font-size: 24px; text-align: center;"><b>SCAN PART</b></span>
  <form onSubmit="App.createPart(); return false;">
      <p>Serial Number:</p><input class="form-control" id="serial_number" type="text">
      <p>Part Type:</p><input class="form-control" id="part_type" type="text">
      <p>Model:</p><input class="form-control" id="model" type="text">
      <p>Manufacturer:</p><input class="form-control" id="manufacturer" type="text">
      <p>Place of manufacture:</p><input class="form-control" id="place" type="text">
      <p><div id="error"> </div>
      <button style="margin-left: 100px;"type="submit" class="btn btn-primary btn-sml custom-btn">SCAN PART</button>
  </form>
</div>
<div class="card card-body order-card">
  <span style="color:#ffab00; font-size: 24px; text-align: center;"><b>SCAN VEHICLE</b></span>
  <form onSubmit="App.createVehicle(); return false;">
      <p>VIN:</p><input class="form-control" id="vin" type="text">
      <p>Manufacturer:</p><input class="form-control" id="vmanufacturer" type="text">
      <p>Model:</p><input class="form-control" id="vmodel" type="text">
      <p>Place of manufacture:</p><input class="form-control" id="vplace" type="text">
      <p><div id="error"> </div>
      <button style="margin-left: 100px;"type="submit" class="btn btn-primary btn-sml custom-btn">SCAN VEHICLE</button>
  </form>
</div>
